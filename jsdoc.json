{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["node_modules/jsdoc/plugins/markdown", "plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "node_modules/docdash",
    "recurse": true,
    "destination": "./docs/",
    "readme": "Home.md",
    "encoding": "utf8"
  }
}
